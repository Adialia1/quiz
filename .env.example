# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Embedding Model Configuration
EMBEDDING_MODEL=intfloat/multilingual-e5-large

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_KEY=your_service_role_key_here

# Clerk Authentication
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your_clerk_publishable_key
CLERK_SECRET_KEY=sk_test_your_clerk_secret_key
CLERK_WEBHOOK_SECRET=whsec_your_webhook_secret
# Optional: Override JWKS URL if auto-detection fails
# CLERK_JWKS_URL=https://your-instance.clerk.accounts.dev/.well-known/jwks.json

# API Configuration
# Backend API URL (for server-side only)
API_URL="https://www.ethicaplus.net"

# Client-side API URL (accessible from React Native app)
# IMPORTANT: Must have EXPO_PUBLIC_ prefix to be accessible in the app
EXPO_PUBLIC_API_URL="https://www.ethicaplus.net"

# Notification API Key
NOTIFICATION_API_KEY=your_notification_api_key_here

# RevenueCat (Monetization)
# Use the Public API key from RevenueCat dashboard
# iOS/Android: appl_xxxxx or goog_xxxxx (NOT the secret key!)
EXPO_PUBLIC_REVENUECAT_API_KEY=appl_bszHXHXrgbpGQdAwAHHELLKLIxQ

# Redis Caching Configuration (Week 1 Optimization)
REDIS_ENABLED=true
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
# REDIS_PASSWORD=  # Optional: Uncomment if Redis requires password

# Async Database Configuration (Week 2 Optimization)
# Get password from Supabase Dashboard > Settings > Database > Connection string
# Format: postgresql://postgres:[PASSWORD]@db.[YOUR_PROJECT_REF].supabase.co:5432/postgres
POSTGRES_URL=postgresql://postgres:[YOUR_DB_PASSWORD]@db.[YOUR_PROJECT_REF].supabase.co:5432/postgres

# Database Connection Pool Settings (optional - uses defaults if not set)
DB_MIN_POOL_SIZE=5
DB_MAX_POOL_SIZE=20
DB_COMMAND_TIMEOUT=60
